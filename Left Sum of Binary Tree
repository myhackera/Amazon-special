def leftSum(root):
    
    summ = 0
    
    def dfs(root):
    
        nonlocal summ
        if root is None:
            return 
        if root.left:
            summ += root.left.val
            
        dfs(root.left)
        dfs(root.right)
        return
        
    dfs(root)
    return summ
